ALL_NAV)},nike.gadget.OneNikeNav.checkNavSize=function(){this.containerElement.find(".tier0").width()>$(window).width()?this.navScrolling||(this.setNavBarScolling(!0),this.navScrolling=!0):this.navScrolling&&(this.setNavBarScolling(!1),this.navScrolling=!1),this.adjustPageTopPadding(this.containerElement.height())},nike.gadget.OneNikeNav.setNavBarScolling=function(e){var n=this.containerElement.find(".tier0"),i=$(window);e?i.bind("scroll",$.throttle(50,nike.gadget.OneNikeNav.repositionBar)):(i.unbind("scroll",nike.gadget.OneNikeNav.repositionBar),n.css("left",0))},nike.gadget.OneNikeNav.repositionBar=function(){if(!Modernizr.touch){var e=nike.gadget.OneNikeNav.containerElement.find(".tier0"),n=$(window);e.width()-n.scrollLeft()>n.width()?e.css("left",n.scrollLeft()*-1):e.css("left",n.width()-e.width())}},nike.gadget.OneNikeNav.updateLogIn=function(){if(nike.exp&&nike.exp.profile&&nike.exp.profile.Login){var e=this.containerElement.find(".logged-in");e.outerWidth()>=160?this.containerElement.find(".oneNike_memberLoggedIn").css({"min-width":e.outerWidth()+"px"}):e.hasClass("is-active")&&e.trigger("click")}},nike.gadget.OneNikeNav.updateViewport=function(){var e=$("meta[name=viewport]");screen.width&&screen.width<=480?e.attr("content","width=device-width"):e.attr("content","width=device-width, initial-scale=1")},nike.gadget.OneNikeNav.openSubNav=function(e,n){var i=this,t=n&&n.parentItemAria||"aria-expanded",a=n&&n.parentItemSelector||".js-rootItem",o=n&&n.subNavContainerSelector||".js-subNavContainer",r=n&&n.subNavAriaHidden||"aria-hidden",s=n&&n.subNavAriaExpanded||"aria-expanded",l=i.containerElement.find('[js-hook="'+e+'"]'),c=l.find(a),d=l.find(o);l.addClass("is-active has--focus").siblings("li").removeClass("is-active has--focus"),c.attr(t,!0),d.attr(r,!1),d.attr(s,!0),i.closeSearchResults()},nike.gadget.OneNikeNav.closeSubNav=function(e){var n=this,i=e&&e.parentItemSelector||".js-rootItem",t=e&&e.rootListItemSelector||".js-rootListItem",a=e&&e.parentItemAria||"a